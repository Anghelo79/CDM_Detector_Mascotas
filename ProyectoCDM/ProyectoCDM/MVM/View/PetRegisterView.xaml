<UserControl x:Class="ProyectoCDM.MVM.View.PetRegisterView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProyectoCDM.MVM.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Background="#1B1B1B">
    <Grid>

        <Label Content="Nombre Mascota" HorizontalAlignment="Left" Height="26" Margin="475,84,0,0" VerticalAlignment="Top" Width="106" Background="{x:Null}" Foreground="White"/>
        <TextBox x:Name="txbnombremascota" HorizontalAlignment="Left" Height="26" Margin="457,144,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="133"/>
        <Button x:Name="btnAgregar" Content="Agregar Mascota" HorizontalAlignment="Left" Height="20" Margin="462,205,0,0" VerticalAlignment="Top" Width="133" Click="Button_Click" IsEnabled="False"/>
        <Button x:Name="BtnGenerar" Content="Generar" HorizontalAlignment="Left" Margin="605,145,0,0" VerticalAlignment="Top" Width="100" Height="29" Click="BtnGenerar_Click" RenderTransformOrigin="0.15,0.669"/>
        <Image x:Name="imgGenrador" HorizontalAlignment="Left" Height="290" Margin="60,65,0,0" VerticalAlignment="Top" Width="365"/>
    </Grid>
</UserControl>
